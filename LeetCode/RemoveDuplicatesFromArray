class Solution {
    public int removeDuplicates(int[] nums) {
        int[] temp = new int[nums.length];
        int i,j=0,k=0;
        int n=nums.length;
        if(n==0){
            return 0;
        }
        for(i=0; i<nums.length-1; i++){
            if (nums[i] != nums[i+1]){
                temp[j] = nums[i];
                j++;
            }
        }
        temp[j]=nums[n-1];
        for(i=0;i<nums.length;i++){
            nums[i]=temp[i];
        }
        return j+1;
    }
}